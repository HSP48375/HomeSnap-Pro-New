      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.naudi.jay.homesnappro"
    },
    "extra": {
      "eas": {
        "projectId": "9b7f0517-c7e8-4978-ab8a-7c2ed2ffe5b4"
      }
    }
  }
